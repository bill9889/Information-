\ifthenelse{\equal{#1}{02}}{February}{}%
\ifthenelse{\equal{#1}{03}}{March}{}%
\ifthenelse{\equal{#1}{04}}{April}{}%
\ifthenelse{\equal{#1}{05}}{May}{}%
\ifthenelse{\equal{#1}{06}}{June}{}%
\ifthenelse{\equal{#1}{07}}{July}{}%
\ifthenelse{\equal{#1}{08}}{August}{}%
